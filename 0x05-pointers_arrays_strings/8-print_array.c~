#include "main.h"
#include <stdio.h>
/**
 * puts2 - put one othwr
 *
 * @str: pointet
 *
 * Return: Always 0.
 */

void puts2(char *str)
{
int i;

for (i = 0; str[i] != '\0'; i++)
{
  if (i % 2 == 0)
  {
	putchar(str[i]);
  }
}
putchar('\n');

}
int main(void)
{
  char *str;

  str = "0123456789";
  puts2(str);
  return (0);
}
